<div id="homePage" class="page">
    <div class="homeButtons">
        <button id="createAccountBtn">Create Account</button>

        <button id="createTransactionBtn">Create Transaction</button>

        <button id="createCategoryBtn">Create Category</button>

        <button id="createBillBtn">Create Bill</button>

        <button id="createIncomeBtn">Create Income</button>
    </div>

    <div id="homeContent">
        <table id="transactions">
            <thead>
                <tr>
                    <th>Date</th>
                    <th>Category</th>
                    <th>Location</th>
                    <th>Amount</th>
                </tr>
            </thead>
            <tbody id="transactionsBody"></tbody>
        </table>

        <div>
            <label class="horLabel">Total Discretionary: $
                <p id="totalDiscretionary"></p>
            </label>

            <label class="horLabel">Remaining Discretionary: $
                <p id="remainingDiscretionary"></p>
            </label>

            <label class="horLabel">Total Income: $
                <p id="totalIncome"></p>
            </label>

            <label class="horLabel">Total Bills: $
                <p id="totalBills"></p>
            </label>
        </div>
    </div>
</div>